{
  "name": "vite-plugin-router-pages",
  "version": "0.0.4",
  "description": "File system base vue-router plugin for Vite",
  "keywords": [
    "vite",
    "vue",
    "vue-router"
  ],
  "license": "MIT",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "pub": "tsx scripts/publish.ts",
    "clean": "pnpm /^clean:/",
    "clean:dist": "rimraf dist",
    "clean:node_modules": "rimraf node_modules"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^2.7.0 || ^3.0.0",
    "vite": "^2.0.0 || ^3.0.0-0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/compiler-sfc": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "@types/debug": "^4.1.7",
    "@vft/constants": "workspace:*",
    "@vft/utils": "workspace:*",
    "@vue/compiler-sfc": "^3.2.45",
    "@vueuse/core": "^9.13.0",
    "cheerio": "1.0.0-rc.12",
    "debug": "^4.3.4",
    "deep-equal": "^2.1.0",
    "extract-comments": "^1.1.0",
    "fast-glob": "^3.2.12",
    "gray-matter": "^4.0.3",
    "json5": "^2.2.3",
    "local-pkg": "^0.4.2",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "picocolors": "^1.0.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/glob-to-regexp": "^0.4.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "*"
      }
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
