{
  "name": "vite-plugin-router-pages",
  "version": "0.0.18",
  "description": "File system base vue-router plugin for Vite",
  "keywords": [
    "vite",
    "vue",
    "vue-router"
  ],
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "main": "dist/index.js",
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "stub": "unbuild --stub",
    "dev": "pnpm stub",
    "build": "pnpm clean:dist && tsup",
    "pub": "pnpm build &&  tsx ../../scripts/build/src/publish.ts --pkg vite-plugin/vite-plugin-router-pages",
    "clean": "pnpm /^clean:/",
    "clean:dist": "rimraf dist",
    "clean:node_modules": "rimraf node_modules"
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "author": {
    "name": "wfly",
    "url": "https://github.com/Miofly",
    "email": "953212389@qq.com"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^2.7.0 || ^3.0.0",
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/compiler-sfc": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "@types/debug": "^4.1.8",
    "@vft/constants": "workspace:*",
    "@vft/utils": "workspace:*",
    "@vue/compiler-sfc": "3.3.4",
    "@vueuse/core": "10.1.2",
    "cheerio": "1.0.0-rc.12",
    "debug": "^4.3.4",
    "deep-equal": "^2.2.1",
    "extract-comments": "^1.1.0",
    "fast-glob": "^3.2.12",
    "gray-matter": "^4.0.3",
    "json5": "^2.2.3",
    "local-pkg": "^0.4.3",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "picocolors": "^1.0.0",
    "yaml": "^2.3.1",
    "@types/deep-equal": "^1.0.1",
    "@types/glob-to-regexp": "^0.4.1",
    "vue": "3.3.4",
    "vue-router": "4.2.1"
  },
  "devDependencies": {
    "unbuild": "2.0.0",
    "@vft/tsconfig": "*"
  }
}
